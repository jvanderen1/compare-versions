{
  "name": "@jvanderen1/compare-versions",
  "version": "1.0.0",
  "description": "Compare semver version strings to find greater, equal or lesser.",
  "keywords": [
    "semver",
    "version",
    "compare",
    "browser",
    "node"
  ],
  "homepage": "https://github.com/jvanderen1/compare-versions#readme",
  "bugs": {
    "url": "https://github.com/jvanderen1/compare-versions/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jvanderen1/compare-versions.git"
  },
  "license": "MIT",
  "author": "<livetoscout@gmail.com>",
  "sideEffects": false,
  "main": "dist/compare-versions.cjs",
  "module": "dist/compare-versions.mjs",
  "source": "src/index.ts",
  "types": "dist/types.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "parcel build --no-source-maps",
    "clean": "pnpm run clean:dist && pnpm run clean:parcel-cache && pnpm run clean:types && pnpm run clean:node-modules",
    "clean-install": "pnpm run clean && pnpm install",
    "clean:dist": "rimraf ./dist/",
    "clean:node-modules": "rimraf ./node_modules/",
    "clean:parcel-cache": "rimraf ./.parcel-cache/",
    "clean:types": "tsc --build --clean",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "pretty": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "pretty:fix": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "test": "c8 --reporter=lcov mocha"
  },
  "devDependencies": {
    "@parcel/core": "2.0.0-nightly.980",
    "@parcel/packager-ts": "2.0.0-nightly.982",
    "@parcel/transformer-typescript-types": "2.0.0-nightly.982",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "c8": "7.11.0",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "mocha": "9.1.4",
    "parcel": "2.0.0-nightly.980",
    "prettier": "2.5.1",
    "typescript": "4.5.4"
  },
  "volta": {
    "node": "16.13.2"
  }
}
